{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "semicolon": [true, "never"],
    "quotemark": [true, "double"],
    "no-console": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "max-classes-per-file": [true, 2],
    "trailing-comma": [false]
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}