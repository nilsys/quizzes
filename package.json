{
  "name": "quizzes",
  "license": "Apache-2.0 OR MIT",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "prettier": "prettier --write \"backend/**/*.js\" \"backend/**/*.ts\"",
    "prettier-check": "prettier  --list-different \"backend/**/*.js\" \"backend/**/*.ts\"",
    "db-migrate": "node --max-old-space-size=16384 dist/backend/src/scripts/quiznator_migrate/index.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "buffer": "^5.1.0",
    "compression": "^1.7.2",
    "dotenv": "^6.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-async-handler": "^1.1.3",
    "express-validator": "^5.2.0",
    "highland": "^2.13.0",
    "keymirror": "^0.1.1",
    "lusca": "^1.6.0",
    "mongoose": "^5.2.9",
    "pg": "^7.4.3",
    "progress": "^2.0.0",
    "reflect-metadata": "^0.1.12",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "typeorm": "^0.3.0-alpha.11",
    "uuid-by-string": "^0.6.0",
    "validate.js": "^0.12.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/jest": "^23.1.2",
    "@types/lusca": "^1.5.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.3.6",
    "@types/nodemailer": "^4.6.2",
    "@types/request": "^2.47.1",
    "@types/sequelize": "^4.27.23",
    "@types/shelljs": "^0.8.0",
    "@types/supertest": "^2.0.4",
    "@types/winston": "^2.3.9",
    "chai": "^4.1.2",
    "concurrently": "^3.6.0",
    "husky": "^1.0.0-rc.9",
    "jest": "^23.1.2",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.6",
    "shelljs": "^0.8.2",
    "supertest": "^3.1.0",
    "ts-jest": "^22.4.6",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.9.2"
  },
  "nodemonConfig": {
    "delay": 500,
    "verbose": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "./bin/check-code-style.sh"
    }
  }
}
